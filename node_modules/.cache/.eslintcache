 {\n            //pehle se like nahi hai ye course\n            //insert karna h ye course liked courses me \n            if(likedCourses.length === 0 ) {\n                setLikedCourses([course.id]);\n            }\n            else {\n                //non-empty pehle se\n                setLikedCourses((prev) => [...prev, course.id]);\n            }\n            toast.success(\"Liked Successfully\");\n        }\n    }\n  return (\n    <div className='w-[300px] bg-bgDark bg-opacity-80 rounded-md overflow-hidden'>\n        <div className='relative'>\n            <img src={course.image.url}/>\n\n            <div className='w-[40px] h-[40px] bg-white rounded-full absolute right-2 bottom-[-12px]\n            grid place-items-center'>\n                <button onClick={clickHandler}>\n                    {\n                        likedCourses.includes(course.id) ? \n                        (<FcLike fontSize=\"1.75rem\" />)\n                        :(<FcLikePlaceholder fontSize=\"1.75rem\" />)\n                    }\n                </button>\n            </div>\n        </div>\n        \n\n        <div className='p-4'>\n            <p className=\"text-white font-semibold text-lg leading-6\">{course.title}</p>\n            <p className='mt-2 text-white'>\n                    {\n                        course.description.length >100 ? \n                        (course.description.substr(0,100)) + \"...\" :\n                        (course.description)\n                    }\n            </p>\n        </div>\n    </div>\n  )\n}\n\nexport default Card\n",{"ruleId":"44","severity":1,"message":"45","line":33,"column":13,"nodeType":"46","endLine":33,"endColumn":42},"jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","JSXOpeningElement"]